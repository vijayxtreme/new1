@media screen and (min-width:600px) and (max-width:991px)
	section.content
		border-top:1px solid #e0e0e0
		
		.main-image
			background:url('../img/feature-image-t.png')
			background-position:0px -0px
			height:559px
			width:100%
			background-repeat:no-repeat
			background-size:cover
			border-bottom:1px solid #e0e0e0
			
			.main-title
				padding-top:123px
				padding-left:20px
				max-width:678px
				font-family:knockout
				color:white
				
				p 
					@extend .futura
					font-size:11px
					letter-spacing:1.5px
					margin-top:30px
					padding-left:0px
					padding-bottom:12px
					font-weight:900
					
				
				h1
					font-size:116px
					line-height:91px
					
				a
					text-decoration:none
					color:white
				

	section.trending-posts
		padding-left:17px
		padding-right:13px
		
		h2
			padding-top:38px
			text-align:center
			font-weight:800
			padding-bottom:34px
			@extend .futura
			font-size:11px
		
		article
			margin-bottom:20px
			box-sizing:content-box
			
			padding-left:20px
			width:calc(50% - 33px)
			
			float:left
			p
				color:white
			
			p.category
				@extend .futura
				font-size:11px
				padding-top:128px
			
			h2.title
				font-family:Georgia
				font-style:italic
				font-weight:500
				letter-spacing:1px
				padding-top:13px
				text-align:left
				color:white
				font-size:25px
				line-height:29px
				width:calc(100% - 40px)
				
			.read-more-trending
				font-family:'Open Sans'
				font-size:10px
				font-weight:800
				letter-spacing:2.0px
		
		article:nth-child(2)
			margin-right:20px

		article:nth-child(4)
			width:calc(100% - 24px)
			margin-right:0px
			
			h2.title
				width:35%
		
		article:nth-child(5)
			margin-right:20px

		article:last-child
			margin-bottom:0px

		article.article1
			background:url('../img/image1-med.png')
			height:362px
			background-size:cover
			background-repeat:no-repeat
		
		article.article2
			background:url('../img/image2-med.png')
			height:362px
			background-size:cover
			background-repeat:no-repeat
			
			h2
				max-width:initial 
		
		article.article3
			background:url('../img/img4.png')
			height:362px
			background-size:cover
			background-repeat:no-repeat
		
		article.article4
			background:url('../img/img5.png')
			height:362px
			margin-bottom:0px
			background-size:cover
			background-repeat:no-repeat
			
		article.article5
			background:url('../img/img6.png')
			height:362px
			background-size:cover
			background-repeat:no-repeat

	section.recent-reviews
		clear:both
		border-bottom:1px solid #e6e6e6
		
		h2
			padding-top:38px
			text-align:center
			padding-bottom:34px
			font-weight:800
			@extend .futura
			font-size:11px

		.recent-main 
			position:relative
			padding-left:33px
			padding-right:19px
			padding-top:37px
			padding-bottom:19px
			width:calc(50% - 53px)
			float:left
			height:235px
			border-right:1px solid #e6e6e6
			border-bottom:1px solid #e6e6e6
			article 
				padding-top:33px
				h3
					clear:both
					padding-top:10px
					font-family: "Georgia"
					font-style:italic
					font-size:20px
					line-height:23px
					text-align:center
					max-width:calc(100% - 30px)
					padding-bottom:18px
					text-decoration:none
					color:#2f2f2f
					
				p 
					font-family:"Open Sans"
					color:#9f9f9f
					font-size:16px
					line-height:21px
					text-align:center
					
				.read-more-link
					margin-top:22px
					font-family:"Open Sans"
					font-size:10px
					letter-spacing:2.0px
					color:black
					font-weight:800
			
			.cat-heading
				@extend .futura
				font-size:11px
				font-weight:800
				position:absolute
				top:26px
				left:34px

			span 
				color:#da4f1f
				padding-bottom:46px
				
			.tiny-logo
				position:absolute
				top:20px
				right:37px
				
		.recent-main:nth-child(odd){
			border-right:none
		}
	
	section.recent-blog-posts
		clear:both

		h2
			text-align:center
			padding-top:21px
			padding-bottom:21px
			@extend .futura
			font-size:11px
			font-weight:800
			border-bottom:1px solid #e6e6e6

		article 
			clear:both
			padding-left:40px
			padding-right:53px
			border-bottom:1px solid #e6e6e6
			min-height:176px
			.article1
				margin-right:44px
				background-image:url('../img/article-b1-t.png')
				background-repeat:no-repeat
				background-size:cover
				height:135px
				width:40.5%
				max-width:203px
				float:left
				
			.article2
				margin-right:44px
				background-image:url('../img/article-b2-t.png')
				background-repeat:no-repeat
				background-size:cover
				height:135px
				width:40.5%
				max-width:203px
				float:left
				
			.article3
				margin-right:44px
				background-image:url('../img/article-b1-t.png')
				background-repeat:no-repeat
				background-size:cover
				height:135px
				width:40.5%
				max-width:203px
				float:left
				
			.article4
				margin-right:44px
				background-image:url('../img/article-b2-t.png')
				background-repeat:no-repeat
				background-size:cover
				height:135px
				width:40.5%
				max-width:203px
				float:left
				

			.cat-heading 
				margin-top:41px
				@extend .futura
				font-size:9px
				display:block
				color:#da4f1f
				font-weight:600
				margin-bottom:15px
				
			h3
				font-size:20.31px
				line-height:23.46px
				text-align:left
				width:calc(100% - 40px)
				padding-bottom:10px
				font-family:"Georgia"
				font-style:italic
			p 
				font-family:"Open Sans"
				text-align:left
				color:#9f9f9f
				width:calc(100% - 60px)
				font-size:14.5px
				line-height:18.58px

			.read-more-link
				margin-top:10px
				font-family:"Open Sans"
				font-size:10px
				letter-spacing:2.0px
				color:black
				font-weight:800
				margin-bottom:32px
	
	section.stay-in-touch
		padding-top:44px
		padding-left:46px
		padding-right:56px
		padding-bottom:31px
		text-align:left
		width:calc(50% - 112px)
		float:left
		
		
		span 
			color:#da4f1f
			@extend .futura
			font-size:12px
			display:block
			padding-bottom:20px
			
		input[type=text], input[type=email]
			width:100%
			border:1px solid #c2c2c2
			margin-bottom:10px
			padding-left:12px
			text-align:left
			height:33px
			font-family:"Open Sans"
			font-size:14.84px	
		
		input[type=submit]
			width:127px
			height:37px
			background-color:#5ca3c4
			border:0px
			color:white
			font-family:"Futura"
			font-weight:800
			font-size:9.84px
			letter-spacing:2px
			margin-top:15px

	section.social-media 

		padding-left:54px
		padding-right:53px
		padding-bottom:47px
		float:left
		width:calc(50% - 107px)
		border-left:1px solid #e6e6e6

		h3 
			margin-top:37px
			color:#da4f1f
			@extend .futura
			font-size:12px
			text-align:left
			padding-bottom:34px
		
		.post
			clear:both
			width:100%
			font-family:"Open Sans"
			font-size:14.5px
			line-height:19px
			
			.social-icon
				float:left
				width:38px
				
			.post-content
				float:left
				width:calc(100% - 51px)
				padding-left:13px
				font-size:14.5px
				line-height:19px
				margin-bottom:25px
				
				h4
					margin-top:-10px
					font-family:"Futura"
					font-size:8px
					letter-spacing:2.0px

				.facebook
					color:#8fb9d4
					
				.twitter
					color:#df91ba
			
	section.social-footer
		clear:both
		border-top:1px solid #e6e6e6 
		position:relative
	
		.s-i
			width:calc(36.7% - 47px)
			padding-top:48px
			max-width:353px
			padding-left:47px
			height:168px
			float:left
			
			.review-logo-black
				background:url('../img/rw-black.png')
				height:83px
				width:165px

		.s-ii
			ul
				position:absolute
				top:-20px
				right:0px

				li 
					display:inline
	
	footer
		padding-top:45px
		padding-left:35px
		width:calc(100% - 70px)
		
		table 
			font-family:"Open Sans"
			font-size:11px
			line-height:17.56px
			color:#494949
			th 
				font-weight:600
				color:#494949
				text-align:left
				padding-bottom:15px
			td 
				text-align:left

			th:last-child, td:last-child
				padding-left:26px
